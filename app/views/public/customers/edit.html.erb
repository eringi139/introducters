<div class="edit">
<h4>会員情報編集</h4>
<%= form_with model: @customer, url: customer_update_customer_path(current_customer), method: :patch do |f| %>

<div>
  <table>
  <tr>
   <th>氏名</th>
   <td><%= f.text_field :last_name %> <%= f.text_field :first_name %></td>
  </tr>
  <tr>
    <th>メールアドレス</th>
    <td><%= f.text_field :email %></td>
  </tr>
  </table>
  <%= f.submit '保存' %>
  <%= link_to "退会する", customer_confirm_withdraw_path(current_customer.id) %>
</div>
<% end %>
</div>
<style>
  div.edit{
    margin-left: 400px;
    margin-top: 20px;
  }
  
</style>